source ~/.mutt-private

alternative_order text/plain text/enriched text/html

auto_view text/html

bind index G imap-fetch-mail

set allow_ansi = yes
set folder = imaps://imap.gmail.com/
set from = "johnsonjh.dev@gmail.com"
set header_cache = "~/.cache/mutt/headers"
set imap_user = "johnsonjh.dev@gmail.com"
set mailcap_sanitize = no
set mbox = "imaps://imap.gmail.com/[Gmail]/All Mail"
set pgp_autosign = yes
set pgp_create_traditional = yes
set pgp_replyencrypt = no
set pgp_replysignencrypted = yes
set pgp_replysign = yes
set pgp_sign_as = 0x6D0799B498DAB092B180075DD7F013C93124B799
set postponed = "imaps://imap.gmail.com/[Gmail]/Drafts"
set realname = "Jeffrey H. Johnson"
set record = "imaps://imap.gmail.com/[Gmail]/Sent Mail"
set smtp_authenticators = "login"
set smtp_url = "smtp://johnsonjh.dev@smtp.gmail.com:587/"
#set spoolfile = "imaps://imap.gmail.com/[Gmail]/All Mail"
set spoolfile = imaps://imap.gmail.com/INBOX
set ssl_force_tls = yes
set ssl_starttls = yes
set tmpdir = "~/.cache/mutt/tmp"
set user_agent = yes
set wait_key = yes

